{
  "name": "Quizlet Chrome Extension",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "app/content-script/content-script.js"
      ]
    }
  ],
  "background": {
    "page": "index.html#/event-page",
    "persistent": false
  },
  "browser_action": {
    "default_title": "Open Popup!",
    "default_popup": "index.html#/popup"
  },
  "icons": {
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}